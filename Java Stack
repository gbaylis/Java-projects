public class Person {
    // name, age, college major and gpa, set to private
    private String name;
    private String age;
    private String major;
    private String gpa;
    
    public Person(String name, String age, String major, String gpa) {
        this.name = name;
        this.age = age;
        this.major = major;
        this.gpa = gpa;
    }
    
    public String toString() {
    // return a statement that tells the information about the student    
        return "Student's Name: " + this.name + " Age: " + this.age + " college major: " + this.major + " gpa: " + this.gpa;
    }
}
